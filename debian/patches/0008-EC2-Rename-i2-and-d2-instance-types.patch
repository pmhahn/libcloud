     'i2.xlarge': {
         'id': 'i2.xlarge',
-        'name': 'High Storage Optimized Extra Large Instance',
+        'name': 'High I/O Storage Optimized Extra Large Instance',
         'ram': GiB(30.5),
         'disk': 800,  # GB
         'bandwidth': None,
@@ -469,7 +469,7 @@ INSTANCE_TYPES = {
